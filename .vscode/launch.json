{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch App Debug",
			"type": "cppvsdbg",
			"request": "launch",
			// change the exe name to your actual exe name
			// (to debug release builds, change `target/debug` to `release/debug`)
			"program": "${workspaceRoot}/src-tauri/target/debug/ann-tool-box.exe",
			"cwd": "${workspaceRoot}/user_data",
			"preLaunchTask": "ui:dev",
			"postDebugTask": "postdebugKill"
		},
	]
}
